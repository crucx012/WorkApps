@for (int i = 0; i <= ViewBag.Pages; i++)
{
    if ((i + 1) == ViewBag.PageIndex)
    {
        <text>@(i + 1)</text>
    }
    else if ((i + 1) < ViewBag.PageIndex - 2)
    {
        if (i == 0)
        {
            <a href="#" onclick=" setPage(@i + 1) ">@(i + 1)</a>
        }
        if (ViewBag.IsElipsis1)
        {
            <text>...</text>
            ViewBag.IsElipsis1 = false;
        }
    }
    else if ((i + 1) > ViewBag.PageIndex + 2)
    {
        if (ViewBag.IsElipsis2)
        {
            <text>...</text>
            ViewBag.IsElipsis2 = false;
        }
        if (i == ViewBag.Pages)
        {
            <a href="#" onclick=" setPage(@i + 1) ">@(i + 1)</a>
        }
    }
    else
    {
        <a href="#" onclick=" setPage(@i + 1) ">@(i + 1)</a>
    }
}